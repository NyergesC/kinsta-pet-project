// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Trip {
    id String  @id @default(uuid())
    destination   String
    description   String
    distance      String
    duration      String
    src           String
    alt           String
  }
